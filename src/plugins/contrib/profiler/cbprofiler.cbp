<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Profiler" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="..\..\..\devel\share\CodeBlocks\plugins\Profiler.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="..\..\..\.objs\2.6\plugins\contrib\profiler" />
				<Option deps_output="..\..\..\.deps\2.6\plugins\contrib\profiler" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="..\..\..\devel\codeblocks.exe" />
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_CFG" value="" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-Winvalid-pch" />
			<Add option="-mthreads" />
			<Add option="-fexceptions" />
			<Add option='-include &quot;sdk.h&quot;' />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DWX_PRECOMP" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DTIXML_USE_STL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="$(#CB.include)" />
			<Add directory="$(#WX.include)" />
			<Add directory="$(#WX)\contrib\include" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add library="codeblocks" />
			<Add library="wxmsw26$(WX_SUFFIX)" />
			<Add directory="$(#CB)\devel" />
			<Add directory="$(#CB.lib)" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel\share\codeblocks\Profiler.zip resources\manifest.xml resources\*.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="cbprofiler.cpp" />
		<Unit filename="cbprofiler.h" />
		<Unit filename="cbprofilerconfig.cpp" />
		<Unit filename="cbprofilerconfig.h" />
		<Unit filename="cbprofilerexec.cpp" />
		<Unit filename="cbprofilerexec.h" />
		<Unit filename="resources\configuration.xrc" />
		<Unit filename="resources\main_dialog.xrc" />
		<Unit filename="resources\manifest.xml" />
		<Extensions>
			<code_completion />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
