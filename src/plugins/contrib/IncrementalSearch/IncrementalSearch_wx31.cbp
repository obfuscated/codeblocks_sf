<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="IncrementalSearch wx3.1.x" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel31/share/codeblocks/plugins/IncrementalSearch" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs31/plugins/contrib/IncrementalSearch" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel31/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DcbDEBUG" />
					<Add option="-DNOPCH" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="../../../include" />
					<Add directory="../../../include/tinyxml" />
					<Add directory="../../../sdk/wxscintilla/include" />
					<Add directory="$(#WX31.include)" />
					<Add directory="$(#WX31.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add library="codeblocks" />
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
					<Add directory="../../../devel31" />
					<Add directory="$(#WX31.lib)/gcc_dll$(WX_CFG)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -jq9 ..\..\..\devel31\share\codeblocks\IncrementalSearch.zip manifest.xml *.xrc" />
					<Add after="zip -Rq9 ..\..\..\devel31\share\codeblocks\IncrementalSearch.zip images\*.png images\16x16\*.png" />
					<Add after="update31.bat" />
					<Mode after="always" />
				</ExtraCommands>
				<Environment>
					<Variable name="WX_CFG" value="" />
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WX_VERSION" value="31" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=gnu++11" />
		</Compiler>
		<Unit filename="IncrementalSearch.cpp" />
		<Unit filename="IncrementalSearch.h" />
		<Unit filename="IncrementalSearchConfDlg.cpp" />
		<Unit filename="IncrementalSearchConfDlg.h" />
		<Unit filename="IncrementalSearchConfDlg.xrc" />
		<Unit filename="IncrementalSearchToolbar.xrc" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
