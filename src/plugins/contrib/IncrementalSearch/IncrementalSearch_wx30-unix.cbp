<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="IncrementalSearch wx3.0.x (Unix)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/codeblocks/plugins/IncrementalSearch" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs30/plugins/contrib/IncrementalSearch" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="../../../devel30/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="`$(WX_CONFIG) --cflags`" />
					<Add option="-Winvalid-pch" />
					<Add option="-fexceptions" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DcbDEBUG" />
					<Add directory="../../../include" />
					<Add directory="../../../sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add option="`$(WX_CONFIG) --libs`" />
					<Add option="-Wl,--no-undefined" />
					<Add library="codeblocks" />
					<Add directory="../../../devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -jq9 ../../../devel30/share/codeblocks/IncrementalSearch.zip manifest.xml *.xrc" />
					<Add after="zip -Rq9 ../../../devel30/share/codeblocks/IncrementalSearch.zip images/*.png images/16x16/*.png" />
					<Add after="./update30" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_CONFIG" value="wx-config --version=3.0" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=c++11" />
			<Add option="-fPIC" />
		</Compiler>
		<Unit filename="IncrementalSearch.cpp" />
		<Unit filename="IncrementalSearch.h" />
		<Unit filename="IncrementalSearchConfDlg.cpp" />
		<Unit filename="IncrementalSearchConfDlg.h" />
		<Unit filename="IncrementalSearchConfDlg.xrc" />
		<Unit filename="IncrementalSearchToolbar.xrc" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
