<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="EditorConfig" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="..\..\..\devel\share\codeblocks\plugins\EditorConfig" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\..\devel" />
				<Option object_output="..\..\..\.objs\plugins\contrib\EditorConfig" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --verbose -p debug" />
				<Option host_application=".\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="..\..\..\include" />
					<Add directory="..\..\..\sdk\wxscintilla\include" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wxmsw28$(WX_SUFFIX)" />
					<Add directory="..\..\..\devel" />
					<Add directory="$(#wx.lib)\gcc_dll$(WX_CFG)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel\share\CodeBlocks\EditorConfig.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=gnu++11" />
		</Compiler>
		<Unit filename="EditorConfig.cpp" />
		<Unit filename="EditorConfig.h" />
		<Unit filename="EditorConfigCommon.cpp" />
		<Unit filename="EditorConfigCommon.h" />
		<Unit filename="EditorConfigUI.cpp" />
		<Unit filename="EditorConfigUI.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith\EditorConfigUI.wxs" />
		<Extensions>
			<code_completion />
			<debugger />
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/EditorConfigUI.wxs" src="EditorConfigUI.cpp" hdr="EditorConfigUI.h" fwddecl="1" i18n="1" name="EditorConfigUI" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
