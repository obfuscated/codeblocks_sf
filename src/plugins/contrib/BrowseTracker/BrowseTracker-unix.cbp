<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="BrowseTracker (Unix)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel/share/codeblocks/plugins/libBrowseTracker" prefix_auto="0" extension_auto="1" />
				<Option object_output="../../../.objs/plugins/contrib/BrowseTracker" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="../../../devel/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="`wx-config --version=2.8 --cflags`" />
					<Add option="-fPIC" />
					<Add option="-DPIC" />
					<Add option="-DNO_LOGGING" />
					<Add option="-DCB_PRECOMP" />
					<Add directory="../../../include" />
					<Add directory="../../../include/tinyxml" />
					<Add directory="../../../sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`wx-config --version=2.8 --libs`" />
					<Add option="-Wl,--no-undefined" />
					<Add library="codeblocks" />
					<Add directory="../../../devel" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ../../../devel/share/codeblocks/BrowseTracker.zip manifest.xml" />
					<Add after="zip -j9 ../../../devel/share/codeblocks/BrowseTracker.zip BrowseTrackerToolbar.xrc" />
					<Add after="zip -9r  ../../../devel/share/codeblocks/BrowseTracker.zip ./images" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wshadow" />
			<Add option="-Winit-self" />
			<Add option="-Wunreachable-code" />
			<Add option="-Wmissing-declarations" />
			<Add option="-Wmissing-include-dirs" />
			<Add option="-Wswitch-enum" />
			<Add option="-Wswitch-default" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-std=c++11" />
		</Compiler>
		<Unit filename="BrowseMarks.cpp" />
		<Unit filename="BrowseMarks.h" />
