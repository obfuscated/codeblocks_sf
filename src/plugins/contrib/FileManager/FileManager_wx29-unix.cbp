<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="File Manager Plugin wx2.9.x (unix)" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel29/share/codeblocks/plugins/FileManager" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs29/plugins/contrib/FileManager" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ni -ns -v" />
				<Option host_application="../../../devel29/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="WX_CONFIG" value="wx-config --version=2.9" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-Ulinux" />
			<Add option="-Uunix" />
			<Add option="`$(WX_CONFIG) --cflags`" />
			<Add option="`pkg-config --cflags gamin`" />
			<Add option="-Winvalid-pch" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-fPIC" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-D__FAM__" />
			<Add directory="../../../include" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="../../../include/mozilla_chardet" />
		</Compiler>
		<Linker>
			<Add option="`$(WX_CONFIG) --libs`" />
			<Add option="`pkg-config --libs gamin`" />
			<Add library="codeblocks" />
			<Add directory="../../../devel29" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel29/share/codeblocks/FileManager.zip manifest.xml Resources/fileexplorersettings.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="FileExplorer.cpp" />
		<Unit filename="FileExplorer.h" />
		<Unit filename="FileExplorerSettings.cpp" />
		<Unit filename="FileExplorerSettings.h" />
		<Unit filename="FileExplorerUpdater.cpp" />
		<Unit filename="FileExplorerUpdater.h" />
		<Unit filename="FileManager.cpp" />
		<Unit filename="FileManager.h" />
		<Unit filename="Resources/fileexplorersettings.xrc" />
		<Unit filename="directorymonitor.cpp" />
		<Unit filename="directorymonitor.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="se_globals.cpp" />
		<Unit filename="se_globals.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
