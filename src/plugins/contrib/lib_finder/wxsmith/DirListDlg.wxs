<resource>
    <object class="wxDialog" name="DirListDlg">
        <title>List of directories with libraries</title>
        <style>wxDEFAULT_DIALOG_STYLE</style>
        <object class="wxFlexGridSizer" variable="FlexGridSizer1" member="yes">
            <rows>0</rows>
            <cols>1</cols>
            <vgap>0</vgap>
            <hgap>0</hgap>
            <object class="sizeritem">
                <option>1</option>
                <border>5</border>
                <flag>wxALL|wxALIGN_CENTER</flag>
                <object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
                    <orient>wxHORIZONTAL</orient>
                    <label>Scanned directories:</label>
                    <object class="sizeritem">
                        <option>1</option>
                        <border>5</border>
                        <flag>wxBOTTOM|wxALIGN_CENTER</flag>
                        <object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="DirList" member="yes">
                            <value></value>
                            <size>292,194</size>
                            <style>wxTE_MULTILINE</style>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_TOP</flag>
                        <object class="wxBoxSizer" variable="BoxSizer1" member="yes">
                            <orient>wxVERTICAL</orient>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_CENTER</flag>
                                <object class="wxButton" name="ID_BUTTON1" variable="Button1" member="no">
                                    <label>Add dir</label>
                                    <handler entry="EVT_BUTTON" function="OnButton1Click" />
                                </object>
                            </object>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxLEFT|wxRIGHT|wxALIGN_CENTER</flag>
                                <object class="wxButton" name="ID_BUTTON2" variable="Button2" member="no">
                                    <label>Clear All</label>
                                    <handler entry="EVT_BUTTON" function="OnButton2Click" />
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
            <object class="sizeritem">
                <option>1</option>
                <flag>wxALIGN_CENTER</flag>
                <object class="wxBoxSizer" variable="BoxSizer2" member="yes">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem">
                        <option>1</option>
                        <border>5</border>
                        <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_CENTER</flag>
                        <object class="wxButton" name="ID_BUTTON3" variable="Button3" member="yes">
                            <label>Cancel</label>
                            <handler entry="EVT_BUTTON" function="OnButton3Click" />
                        </object>
                    </object>
                    <object class="sizeritem">
                        <option>1</option>
                        <border>5</border>
                        <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_CENTER</flag>
                        <object class="wxButton" name="ID_BUTTON4" variable="Button4" member="yes">
                            <label>Next</label>
                            <default>1</default>
                            <handler entry="EVT_BUTTON" function="OnButton4Click" />
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
