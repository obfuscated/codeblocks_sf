				</Compiler>
				<Linker>
					<Add library="wxmsw28u" />
					<Add directory="$(#wx.lib)\gcc_dll" />
				</Linker>
			</Target>
			<Target title="wxSpellChecker">
				<Option output="..\..\..\devel\wxSpellChecker" prefix_auto="1" extension_auto="1" />
