					<Add library="wxmsw28u" />
					<Add directory="..\..\..\devel" />
					<Add directory="$(#wx.lib)\gcc_dll" />
				</Linker>
			</Target>
			<Target title="plugin">
				<Option output="..\..\..\devel\share\CodeBlocks\plugins\SpellChecker" prefix_auto="1" extension_auto="1" />
				<Option object_output="..\..\..\.objs\plugins\contrib\SpellChecker" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-check-associations -ns -nd --multiple-instance" />
				<Option host_application="..\..\..\devel\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
