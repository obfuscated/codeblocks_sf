<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="C::B KeyBinder" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="..\..\..\devel\share\CodeBlocks\plugins\keybinder.dll" prefix_auto="0" extension_auto="0" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="..\..\..\devel\codeblocks.exe" />
				<Compiler>
					<Add option="-Winvalid-pch" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option='-include &quot;sdk.h&quot;' />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DTIXML_USE_STL" />
					<Add option="-DwxUSE_UNICODE" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_CFG" value="" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add directory="$(#CB.include)" />
			<Add directory="$(#CB.include)\wxscintilla\include" />
			<Add directory="$(#WX.include)" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
			<Add directory="$(#WX)\contrib\include" />
		</Compiler>
		<Linker>
			<Add library="codeblocks" />
			<Add library="wxmsw26$(WX_SUFFIX)" />
			<Add directory="$(#CB)\devel" />
			<Add directory="$(#CB.lib)" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel\share\CodeBlocks\keybinder.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="cbkeybinder.cpp" />
		<Unit filename="cbkeybinder.h" />
		<Unit filename="debugging.h" />
		<Unit filename="keybinder.cpp" />
		<Unit filename="keybinder.h" />
		<Unit filename="keybinderdef.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="menuutils.cpp" />
		<Unit filename="menuutils.h" />
		<Extensions>
			<code_completion />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
