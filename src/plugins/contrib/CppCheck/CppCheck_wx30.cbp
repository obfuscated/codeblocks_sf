<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CppCheck/Vera++ wx3.0.x" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/codeblocks/plugins/CppCheck.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs30/plugins/contrib/CppCheck" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Option projectCompilerOptionsRelation="0" />
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="-mthreads" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DNOPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-std=gnu++11" />
			<Add directory="../../../include" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="$(#WX30.INCLUDE)" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="../../../devel30" />
			<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel30/share/codeblocks/CppCheck.zip resources/manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="ConfigPanel.cpp" />
		<Unit filename="ConfigPanel.h" />
		<Unit filename="CppCheck.cpp" />
		<Unit filename="CppCheck.h" />
		<Unit filename="CppCheckListLog.cpp" />
		<Unit filename="CppCheckListLog.h" />
		<Unit filename="wxsmith/ConfigPanel.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/ConfigPanel.wxs" src="ConfigPanel.cpp" hdr="ConfigPanel.h" fwddecl="1" i18n="1" name="ConfigPanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
